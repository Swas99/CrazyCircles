<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item  >
        <shape android:shape="rectangle"
            >
            <stroke android:width="6dp" android:color="#FFFFFF" />
            <padding android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp"
                />
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"
            >
            <stroke android:width="3dp" android:color="#BA1111"/>
            <padding android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"
                />
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>




    <item >
        <shape android:shape="oval">

            <solid android:color="#11AAAAAA"
                />
        </shape>
    </item>


</layer-list>